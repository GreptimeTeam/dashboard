{
  "name": "greptime_dashboard",
  "description": "dashboard for GreptimeDB",
  "version": "1.0.0",
  "private": true,
  "author": "Greptime",
  "license": "Apache-2.0",
  "scripts": {
    "dev": "vite --config ./config/vite.config.dev.ts",
    "dev:cloud": "vite --mode cloud --config ./config/vite.config.dev.ts ",
    "build": "vite build --config ./config/vite.config.prod.ts && vue-tsc --noEmit",
    "build:docker": "vite build --config ./config/vite.config.prod.ts --base /dashboard/ && vue-tsc --noEmit",
    "build:cloud": "vite build --config ./config/vite.config.prod.ts --mode cloud --base /dashboard/ && vue-tsc --noEmit",
    "build:staging": "vite build --config ./config/vite.config.prod.ts --mode staging --base /dashboard/ && vue-tsc --noEmit",
    "report": "cross-env REPORT=true npm run build",
    "preview": "npm run build && vite preview --host --config ./config/vite.config.base.ts",
    "preview:cloud": "npm run build:cloud && vite preview --host --config ./config/vite.config.base.ts",
    "type:check": "vue-tsc --noEmit --skipLibCheck",
    "lint-staged": "npx lint-staged",
    "prepare": "husky install",
    "update:lock": "npm i --package-lock-only;pnpm i --lockfile-only"
  },
  "lint-staged": {
    "*.{js,ts,jsx,tsx}": [
      "prettier --write"
    ],
    "*.vue": [
      "prettier --write"
    ],
    "*.{less,css}": [
      "prettier --write"
    ]
  },
  "dependencies": {
    "@arco-design/web-vue": "^2.41.0",
    "@babel/core": "^7.20.5",
    "@codemirror/lang-python": "^6.1.0",
    "@codemirror/lang-sql": "^6.3.1",
    "@codemirror/theme-one-dark": "^6.1.0",
    "@vueuse/core": "^9.6.0",
    "arco-design-pro-vue": "^2.6.1",
    "axios": "^0.27.2",
    "codemirror": "6.x",
    "consola": "^2.15.3",
    "cross-storage": "^1.0.0",
    "dayjs": "^1.11.7",
    "echarts": "^5.4.1",
    "lodash": "^4.17.21",
    "mitt": "^3.0.0",
    "nprogress": "^0.2.0",
    "pinia": "^2.0.29",
    "qs": "^6.11.0",
    "query-string": "^8.1.0",
    "sortablejs": "^1.15.0",
    "stylelint-config-rational-order-fix": "^0.1.9",
    "stylelint-config-recommended-less": "^1.0.4",
    "stylelint-config-recommended-vue": "^1.4.0",
    "stylelint-stylus": "^0.18.0",
    "ts-md5": "^1.3.1",
    "vue": "^3.2.47",
    "vue-codemirror": "^6.1.1",
    "vue-echarts": "^6.5.0",
    "vue-i18n": "^9.2.2",
    "vue-router": "^4.0.14"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.4.2",
    "@commitlint/config-conventional": "^17.3.0",
    "@prettier/plugin-pug": "^2.4.1",
    "@types/cross-storage": "^0.8.30",
    "@types/lodash": "^4.14.191",
    "@types/mockjs": "^1.0.7",
    "@types/node": "^18.11.18",
    "@types/nprogress": "^0.2.0",
    "@types/qs": "^6.9.7",
    "@types/sortablejs": "^1.15.0",
    "@typescript-eslint/eslint-plugin": "^5.54.1",
    "@typescript-eslint/parser": "^5.53.0",
    "@vitejs/plugin-vue": "^3.1.2",
    "@vitejs/plugin-vue-jsx": "^2.1.1",
    "@volar/vue-language-plugin-pug": "^1.0.24",
    "@vue/babel-plugin-jsx": "^1.1.1",
    "cross-env": "^7.0.3",
    "eslint": "^8.31.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-import-resolver-typescript": "^3.5.3",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-vue": "^9.8.0",
    "husky": "^8.0.3",
    "less": "^4.1.3",
    "lint-staged": "^13.2.0",
    "mockjs": "^1.1.0",
    "prettier": "^2.7.1",
    "pug": "^3.0.2",
    "rollup": "^3.7.5",
    "rollup-plugin-visualizer": "^5.8.2",
    "stylelint": "^14.16.0",
    "stylelint-config-prettier": "^9.0.3",
    "stylelint-config-standard": "^29.0.0",
    "stylelint-order": "^5.0.0",
    "stylus": "^0.59.0",
    "typescript": "^4.8.4",
    "unplugin-auto-import": "^0.12.1",
    "unplugin-vue-components": "^0.24.0",
    "vite": "^3.1.7",
    "vite-plugin-compression": "^0.5.1",
    "vite-plugin-eslint": "^1.8.1",
    "vite-plugin-style-import": "2.0.0",
    "vite-svg-loader": "^4.0.0",
    "vue-tsc": "^1.0.19"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "resolutions": {
    "bin-wrapper": "npm:bin-wrapper-china",
    "rollup": "^2.56.3",
    "gifsicle": "5.2.0"
  }
}
