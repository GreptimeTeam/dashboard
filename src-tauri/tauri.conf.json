{
  "$schema": "../node_modules/@tauri-apps/cli/config.schema.json",
  "productName": "greptime_dashboard",
  "version": "0.1.0",
  "identifier": "com.greptime",
  "build": {
    "frontendDist": "../dist",
    "devUrl": "http://localhost:5177",
    "beforeDevCommand": "pnpm edge",
    "beforeBuildCommand": "pnpm build:dashboard-edge"
  },
  "app": {
    "windows": [
      {
        "title": "GreptimeDashboard",
        "width": 800,
        "height": 600,
        "resizable": true,
        "fullscreen": false
      }
    ],
    "security": {
      "csp": null
    }
  },

  "bundle": {
    "active": true,
    "targets": "all",
    "icon": ["icons/32x32.png", "icons/128x128.png", "icons/128x128@2x.png", "icons/icon.icns", "icons/icon.ico"]
  }
}
